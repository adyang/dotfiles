syntax on
filetype plugin indent on

set nocompatible
set number
set ruler
set encoding=utf-8
set tabstop=2 shiftwidth=2 expandtab
set autoindent
set backspace=indent,eol,start

colorscheme base16-oceanicnext
" colorscheme base16-cupertino

nnoremap Q <Nop>

let g:netrw_altv=1
let g:netrw_alto=1
let g:netrw_liststyle=3
